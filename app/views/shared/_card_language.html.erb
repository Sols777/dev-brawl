<div class="col accordion-item">
  <h2 class="accordion-header">
    <button class="accordion-button" type="button" data-bs-toggle="collapse" data-bs-target="#collapse<%= index%>" aria-expanded="true" aria-controls="collapseOne">
      <%= language.name %>
    </button>
  </h2>
  <div id="collapse<%= index%>" class="accordion-collapse collapse" data-bs-parent="#accordionExample">
    <div class="accordion-body">
    <%= link_to (language.challenges.sample.name if language.challenges.size > 0) , "/languages/#{language.id}/submissions/new" %>
      <%# <%= language.challenges.sample.name if language.challenges.size > 0 %>
    </div>
  </div>
</div>
